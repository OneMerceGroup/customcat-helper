{
    "manifest_version": 3,
    "name": "OMG - Customcat helper",
    "version": "0.0.1",
    "description": "Customcat helper", 
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["*://*.customcat.com/*"],
        "js": ["content.js"]
      }
    ]
}